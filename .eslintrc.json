{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "process": true,
    "setInterval": true
  },
  "rules": {
    "curly": "error",
    "dot-notation": "error",
    "eqeqeq": "error",
    "no-console": 0,
    "no-floating-decimal": "error",
    "no-redeclare": "error",
    "no-regex-spaces": "error",
    "no-self-assign": "error",
    "no-unsafe-negation": "error",
    "no-var": "error",
    "no-process-env": 0,
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1
      }
    ]
  }
}
